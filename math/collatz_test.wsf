    "math/collatz" import

collatz_test:
    1
.collatz_test_loop:
    ^ call math.collatz
    printi '\n' printc
    1+
    ^ 73 j= .collatz_test_end
    jmp .collatz_test_loop
.collatz_test_end:
    drop end
