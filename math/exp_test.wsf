# (x y -- )
test_exp:
    -1 '\n'
    ^3 ^3 call exp -2
    " = "
    ^7 -2
    " ** "
    ^14 -2
    call string.printf
    2drop ret

# (x y m -- )
test_modexp:
    -1
    ")\n"
    ^3 -2
    " (mod "
    ^13 ^13 ^13 call modexp -2
    " = "
    ^17 -2
    " ** "
    ^24 -2
    call string.printf
    3drop ret

# (x y -- )
test_tetrate:
    -1
    '\n'
    ^3 ^3 call tetrate -2
    " = "
    ^7 -2
    " ^^ "
    ^14 -2
    call string.printf
    2drop ret

# (x y -- )
test_pentate:
